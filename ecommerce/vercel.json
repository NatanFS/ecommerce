{
    "builds": [{
        "src": "build_staticfiles.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      },
      {
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
        {
            "src": "/(.*)",
            "dest": "ecommerce/wsgi.py"
        }
    ]
}
